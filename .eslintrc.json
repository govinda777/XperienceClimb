{
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "prefer-const": "error",
    "no-unused-vars": [
      "error",
      {
        "args": "after-used",
        "ignoreRestSiblings": false,
        "argsIgnorePattern": "^_.*?$"
      }
    ]
  },
  "ignorePatterns": ["node_modules/", ".next/", "out/", "build/", "*.config.js", "*.setup.js"]
}
